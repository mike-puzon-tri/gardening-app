{
  "name": "tri-web-app",
  "description": "This is a boilerplate build with Vite, React 18, TypeScript, Vitest, Testing Library, TailwindCSS 3, Eslint and Prettier.",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --max-warnings=0",
    "lint-fix": "eslint --fix src --ext ts,tsx .",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@aws-amplify/ui-react": "^5.3.2",
    "@dnd-kit/core": "^6.1.0",
    "@material-tailwind/react": "^2.1.9",
    "@tanstack/react-query": "^5.35.1",
    "@toyota-research-institute/rse-react-library": "^0.0.16",
    "aws-amplify": "^5.3.12",
    "axios": "^1.6.8",
    "graphql": "^16.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.6",
    "@types/node": "^20.12.10",
    "@types/react": "18.2.19",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "happy-dom": "^14.7.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vite-plugin-graphql-server": "^1.2.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
